<%-include('../partials/head')%>
<%-include('../partials/navbarcontain')%>
<div class="container mt-5">
<H1 class="text-center mb-4"> Contact Us!</H1>

<div class="col-md-10 mx-auto">
<form action="https://formsubmit.co/e8d797a5f7d4f01056d40c68fd93c180" method="POST">
    <input type="text" name="_honey" style="display: none;">

    <input type="hidden" name="_captcha" value="false">
    <div class="row g-3 justify-content-center">
    <div class="col-md-6">
        <label for="firstName" class="form-label">First Name</label>
        <input class="form-control" id="firstName" name="First Name" type="text" required>
    </div>
    <div class="col-md-6" >
        <label for="lastName" class="form-label">Last Name</label>
        <input class="form-control" name="Last Name" id="lastName" type="text" style="padding-left: 10px" required>
    </div>
    </div>
       <div class="row g-3 justify-content-center">
    <div class="col-md-6">
        <label for="replyEmail" class="form-label">Email Address</label>
        <input class="form-control" name="Enail Address" id="replyEmail" type="text" required>
    </div>
    <div class="col-md-6" >
        <label for="phoneNumber" class="form-label">Phone Number</label>
        <input class="form-control" id="phoneNumber" name="Phone Number" type="text" style="padding-left: 10px" required>
    </div>
    </div>
    <div class="col-md-12">
        <label for="emailSub" class="form-label">Email Inquiry</label>
        <textarea name="Email Body" class="form-control" id="emailSub" rows="10" type="text" required>

        </textarea>
    </div>
    <div class="d-flex justify-content-center">
    <button class="btn btn-danger col-md-3 mx-auto">Email an Inquiry</button>
    </div>
</form>
</div>
</div>